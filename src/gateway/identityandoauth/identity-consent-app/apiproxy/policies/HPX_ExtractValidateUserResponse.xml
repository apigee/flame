<ExtractVariables name="HPX_ExtractValidateUserResponse">
    <DisplayName>ExtractValidateUserResponse</DisplayName>
    <Source>validateUserResponse</Source>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <JSONPayload>
        <Variable name="userid">
            <JSONPath>$.user.username</JSONPath>
        </Variable>
        <Variable name="patient_id">
            <JSONPath>$.user.patient_id</JSONPath>
        </Variable>
    </JSONPayload>
</ExtractVariables>
